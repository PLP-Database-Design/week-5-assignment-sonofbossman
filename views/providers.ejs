<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>provider_specialty</th>
        </tr>  
    </thead>
    <tbody>
        <% providers.forEach(row =>{%>
            <tr>
                <td> <%= row.first_name %> </td>
                <td> <%= row.last_name %></td>
                <td> <%= row.provider_specialty %></td>
            </tr>
        <%});%>
    </tbody>
</table>